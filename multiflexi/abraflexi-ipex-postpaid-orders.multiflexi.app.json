{
    "name": "Postpaid IPEX to AbraFlexi Orders",
    "description": "Generate AbraFlexi orders from postpaid Ipex calls",
    "executable": "abraflexi-ipex-postpaid-orders",
    "setup": "abraflexi-ipex-setup",
    "cmdparams": "",
    "deploy": "apt install abraflexi-ipex",
    "homepage": "https://github.com/Spoje-NET/abraflexi-ipex",
    "requirements": ["AbraFlexi"],
    "ociimage": "",
    "topics": ["AbraFlexi", "Ipex", "Invoice"],
    "uuid": "51b247aa-fd48-44d7-8494-8c86b05e5a66",
    "produces": {
        "orders": {
            "description": "AbraFlexi orders created from IPEX calls",
            "format": "json"
        }
    },
    "environment": {
        "APP_DEBUG": {
            "type": "bool",
            "description": "Show debug messages",
            "defval": "False",
            "required": false
        },
        "RESULT_FILE": {
            "type": "string",
            "description": "write output json data to",
            "defval": "multiflexi_ipex_orders_{ABRAFLEXI_COMPANY}.json",
            "required": false
        },
        "EASE_LOGGER": {
            "type": "string",
            "description": "Logger type",
            "defval": "syslog|console",
            "required": false
        },
        "EMAIL_FROM": {
            "type": "string",
            "description": "Email from address",
            "defval": "michal.tomasek@spojenet.cz",
            "required": true
        },
        "SEND_INFO_TO": {
            "type": "string",
            "description": "Send info to address",
            "defval": "",
            "required": true
        },
        "ABRAFLEXI_URL": {
            "type": "string",
            "description": "AbraFlexi URL",
            "defval": "",
            "required": true
        },
        "ABRAFLEXI_LOGIN": {
            "type": "string",
            "description": "AbraFlexi login",
            "defval": "",
            "required": true
        },
        "ABRAFLEXI_PASSWORD": {
            "type": "string",
            "description": "AbraFlexi password",
            "defval": "",
            "required": true
        },
        "ABRAFLEXI_COMPANY": {
            "type": "string",
            "description": "AbraFlexi company",
            "defval": "",
            "required": true
        },
        "ABRAFLEXI_ORDERTYPE": {
            "type": "string",
            "description": "AbraFlexi order type",
            "defval": "code:OBP_VOIP",
            "required": true
        },
        "ABRAFLEXI_PRODUCT": {
            "type": "string",
            "description": "AbraFlexi product",
            "defval": "code:IPEX_POSTPAID",
            "required": true
        },
        "ABRAFLEXI_DOCTYPE": {
            "type": "string",
            "description": "AbraFlexi document type",
            "defval": "code:FAKTURA",
            "required": true
        },
        "ABRAFLEXI_CREATE_EMPTY_ORDERS": {
            "type": "bool",
            "description": "Create empty orders",
            "defval": "False",
            "required": false
        },
        "ABRAFLEXI_SKIPLIST": {
            "type": "string",
            "description": "AbraFlexi skip list",
            "defval": "",
            "required": false
        },
        "IPEX_URL": {
            "type": "string",
            "description": "Ipex URL",
            "defval": "https://restapi.ipex.cz",
            "required": true
        },
        "IPEX_LOGIN": {
            "type": "string",
            "description": "Ipex login",
            "defval": "",
            "required": true
        },
        "IPEX_PASSWORD": {
            "type": "string",
            "description": "Ipex password",
            "defval": "",
            "required": true
        },
        "MONTH_OFFSET": {
            "type": "int",
            "description": "Month offset for processing (negative values for past months, e.g. -1 for last month)",
            "defval": -1,
            "required": false
        },
        "ATTACH_CALL_LIST_PDF": {
            "type": "bool",
            "description": "Enable attaching PDF call list to orders in AbraFlexi",
            "defval": "True",
            "required": false
        },
        "SEND_CALL_LIST_EMAIL": {
            "type": "bool",
            "description": "Enable sending call list PDF by email to customers",
            "defval": "True",
            "required": false
        }
    },
    "multiflexi": "1.20.0"
}
